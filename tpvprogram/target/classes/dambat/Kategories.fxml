<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="dambat.kategories.KategoriesController">
    <children>
        <TableView fx:id="kategoriestable" layoutX="0.0" layoutY="0.0">
            <columns>
               <TableColumn fx:id="colId" text="ID"/>
               <TableColumn fx:id="colName" text="  kategoria Name"/>
            </columns>
        </TableView>
        <Button fx:id="myButton" layoutX="150.0" layoutY="300.0" text="Load Products" onAction="#switchToProducts"/>
    </children>
</AnchorPane>
